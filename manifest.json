{\n  \"manifest_version\": 3,\n  \"name\": \"n9n AI Assistant\",\n  \"version\": \"1.0.0\",\n  \"description\": \"AI-powered chatbot extension for enhanced browsing\",\n  \"permissions\": [\n    \"activeTab\",\n    \"storage\",\n    \"identity\"\n  ],\n  \"host_permissions\": [\n    \"https://*.supabase.co/*\"\n  ],\n  \"background\": {\n    \"service_worker\": \"src/background/background.ts\",\n    \"type\": \"module\"\n  },\n  \"content_scripts\": [\n    {\n      \"matches\": [\"<all_urls>\"],\n      \"js\": [\"src/content/content.ts\"]\n    }\n  ],\n  \"action\": {\n    \"default_popup\": \"src/popup/popup.html\",\n    \"default_title\": \"n9n AI Assistant\"\n  },\n  \"web_accessible_resources\": [\n    {\n      \"resources\": [\"src/content/*\"],\n      \"matches\": [\"<all_urls>\"]\n    }\n  ]\n}